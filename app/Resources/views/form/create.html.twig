<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>

<h1>Create</h1>


{% block body %}


    {{ form_start(form, { 'action': path('entry_create'), 'method': 'POST', 'attr': {'class': 'Article'} }) }}
    {{ form_errors(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.createdAt) }}

    {{ form_rest(form) }}

    <input type="submit" value="Submit" />

{% endblock %}
</body>
</html>